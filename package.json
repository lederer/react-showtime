{
  "name": "react-showtime",
  "version": "0.2.0",
  "description": "Mount and unmount with CSS transitions",
  "author": "Scott Lederer",
  "license": "Apache-2.0",
  "repository": "lederer/react-showtime",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "index.d.ts",
  "jsnext:main": "dist/index.es.js",
  "files": [
    "dist/*.js",
    "dist/*.map"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=5"
  },
  "bugs": {
    "url": "https://github.com/lederer/react-showtime/issues"
  },
  "scripts": {
    "test": "yarn node --experimental-vm-modules $(yarn bin jest) --env=jsdom",
    "test:watch": "yarn node --experimental-vm-modules $(yarn bin jest) --env=jsdom --watchAll",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "deploy": "yarn install && yarn run build && cd example && yarn install && yarn run build",
    "lint": "eslint --ext .js,.jsx src example/src",
    "compile": "tsc"
